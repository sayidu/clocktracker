<div> 
  <p>Clock In/Out Application</p>

  <div>
    @user_time_logs.map do |log|
      <%= log.organization.name %>  
    end 
  </div>

  <% if user_signed_in? %>
    <li>
    <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
    </li>
    <% else %>
    <li>
    <%= link_to('Login', new_user_session_path) %>  
    </li>
  <% end %>
</div>